- if @organization.projects.archived.any?
  %h3= t('.title')
  .projects
    - @organization.projects.archived.each do |project|
      %p.project
        = link_to h(project), project
        %span
          = t('.started_closed_at_html', :date_start => project.date_start.to_date, :date_end => project.date_end.to_date)  unless project.date_start.nil? || project.date_end.nil?
        %span.status
          = t('.last_activity', :date => project.updated_at.to_date)
